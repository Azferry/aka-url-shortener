{
    "python.pythonPath": "Scripts/python.exe",
    "files.exclude" : {
        "**/.git" : true,
        // "**/.gitignore" : true,
        "Lib" : true,
        "lib" : true,
        "Include" : true,
        "Scripts" : true,
        "pyvenv.cfg": true,
        "**/__pycache__": true
    },
    "cSpell.enableFiletypes": [
        "!python"
    ],
    "python.analysis.extraPaths": [
        "./src/api/data",
        "./src/api/data"
    ],
    "appService.zipIgnorePattern": [
        "__pycache__{,/**}",
        "*.py[cod]",
        "*$py.class",
        ".Python{,/**}",
        "build{,/**}",
        "develop-eggs{,/**}",
        "dist{,/**}",
        "downloads{,/**}",
        "eggs{,/**}",
        ".eggs{,/**}",
        "lib{,/**}",
        "lib64{,/**}",
        "parts{,/**}",
        "sdist{,/**}",
        "var{,/**}",
        "wheels{,/**}",
        "share/python-wheels{,/**}",
        "*.egg-info{,/**}",
        ".installed.cfg",
        "*.egg",
        "MANIFEST",
        ".env{,/**}",
        ".venv{,/**}",
        "env{,/**}",
        "venv{,/**}",
        "ENV{,/**}",
        "env.bak{,/**}",
        "venv.bak{,/**}",
        ".vscode{,/**}"
    ]
}
